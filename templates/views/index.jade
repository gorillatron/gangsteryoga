extends ../layouts/default
include ../mixins/vscript


block js
  +vscript('front-page')

block content

  include ./subpages/fengsler
  include ./subpages/instruktoerer
  include ./subpages/vaartarbeid
  include ./subpages/yogasommetode


  #front-page.relative


    header#main-header.relative.fit.p3(style="background-image: url(/images/webgangster_14.jpg);")
      .logo.left
      nav#menu.right.right-align
        ul.list-reset
          li.mb1
            a(href="/vaartarbeid") Om oss
          li.mb1
            a(href="/") Instruktører
          li.mb1
            a(href="/") Fengsler
          li.mb1
            a(href="/") Kontakt
      
      section#featured.clearfix.absolute.bottom-0.left-0.mb1.mx1
        .text.col.col-7 
          .content.p3 Gangster Yoga setter søkelys på det å være menneske og hvordan man får kontakt med det innerste i seg selv. Puste seg gjennom utfordringer. Kjenne på stivheter og ubehag, og gradvis bygge opp en større klarhet i sinnet som blir vedvarende og stabil.
        .video.col.col-5
          .content
            iframe.hide(src='https://player.vimeo.com/video/179913310?title=0&byline=0&portrait=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')

    

    section#content.clearfix.fit

      section.clearfix.my1
        
        #map.col.col-8 map

        #articles.col.col-4.center

